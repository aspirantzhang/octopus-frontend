{
  "name": "octopus-frontend",
  "version": "5.0.0-beta.3",
  "private": true,
  "description": "",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "openapi": "umi openapi",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "cross-env UMI_ENV=dev umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc --noEmit",
    "e2e:html": "nyc report --reporter=html",
    "e2e:cov": "nyc report --reporter=text-lcov > coverage.lcov",
    "all": "yarn lint:fix && yarn lint && yarn test:all"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.6.4",
    "@ant-design/pro-descriptions": "^1.9.8",
    "@ant-design/pro-form": "^1.37.0",
    "@ant-design/pro-layout": "^6.24.3",
    "@ant-design/pro-table": "^2.50.1",
    "@formily/antd": "^2.0.0-rc.4",
    "@formily/core": "^2.0.0-rc.4",
    "@formily/react": "^2.0.0-rc.4",
    "@tinymce/tinymce-react": "^3.12.6",
    "@umijs/plugin-openapi": "^1.0.1",
    "@umijs/route-utils": "^1.0.36",
    "ahooks": "^2.10.9",
    "antd": "^4.16.13",
    "classnames": "^2.2.6",
    "lodash": "^4.17.11",
    "moment": "^2.25.3",
    "omit.js": "^2.0.2",
    "qs": "^6.9.0",
    "query-string": "^7.0.1",
    "rc-queue-anim": "^2.0.0",
    "react": "^17.0.0",
    "react-dev-inspector": "^1.6.0",
    "react-dom": "^17.0.0",
    "react-flag-icon-css": "git+https://github.com/ryanskidmore/react-flag-icon-css",
    "react-helmet-async": "^1.1.2",
    "swagger-ui-react": "^4.0.0-beta.4",
    "umi": "^3.5.17",
    "umi-request": "^1.0.8"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.13",
    "@types/history": "^4.7.9",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-flag-icon-css": "^1.0.5",
    "@types/react-helmet": "^6.1.2",
    "@types/react-test-renderer": "^17.0.1",
    "@types/swagger-ui-react": "^3.35.2",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@umijs/fabric": "^2.6.2",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/plugin-esbuild": "^1.0.1",
    "@umijs/preset-ant-design-pro": "^1.3.3",
    "@umijs/preset-dumi": "^1.1.25",
    "@umijs/preset-react": "^1.8.22",
    "@umijs/yorkie": "^2.0.3",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "detect-installer": "^1.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^35.0.0",
    "express": "^4.17.1",
    "gh-pages": "^3.2.3",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^11.1.2",
    "mockjs": "^1.0.1-beta3",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "puppeteer": "^10.2.0",
    "puppeteer-to-istanbul": "^1.4.0",
    "react-test-renderer": "^17.0.2",
    "standard": "^16.0.3",
    "stylelint": "^13.0.0",
    "typescript": "^4.4.2"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
