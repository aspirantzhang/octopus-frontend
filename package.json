{
  "name": "octopus-frontend",
  "version": "5.2",
  "private": true,
  "description": "",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --custom-syntax postcss-less",
    "openapi": "umi openapi",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "serve": "umi-serve",
    "start": "cross-env UMI_ENV=dev umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
    "test": "umi test",
    "test:component": "umi test ./src/components",
    "test:e2e": "node ./tests/run-tests.js",
    "tsc": "tsc --noEmit",
    "e2e:html": "nyc report --reporter=html",
    "e2e:cov": "nyc report --reporter=text-lcov > coverage.lcov",
    "all": "yarn lint:fix && yarn lint && yarn test:all"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-descriptions": "^1.10.29",
    "@ant-design/pro-form": "^1.52.7",
    "@ant-design/pro-layout": "^6.32.3",
    "@ant-design/pro-table": "^2.62.1",
    "@formily/antd": "^2.0.8",
    "@formily/core": "^2.0.8",
    "@formily/react": "^2.0.8",
    "@tinymce/tinymce-react": "^3.13.0",
    "@umijs/route-utils": "^2.0.4",
    "ahooks": "^3.1.5",
    "antd": "^4.17.0",
    "classnames": "^2.3.0",
    "country-flag-icons": "^1.4.19",
    "lodash": "^4.17.11",
    "moment": "^2.29.0",
    "omit.js": "^2.0.2",
    "postcss": "8",
    "qs": "^6.10.2",
    "query-string": "^7.1.0",
    "rc-menu": "^9.0.13",
    "rc-queue-anim": "^2.0.0",
    "rc-util": "^5.14.0",
    "react": "^17.0.0",
    "react-dev-inspector": "^1.7.1",
    "react-dom": "^17.0.0",
    "react-helmet-async": "^1.2.2",
    "swagger-ui-react": "^4.1.3",
    "umi": "^3.5.0",
    "umi-plugin-antd-theme": "^2.1.3",
    "umi-plugin-pro": "^2.0.1",
    "umi-request": "^1.4.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.5",
    "@babel/core": "^7.16.12",
    "@babel/eslint-parser": "^7.16.5",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/classnames": "^2.2.7",
    "@types/expect-puppeteer": "^4.4.7",
    "@types/express": "^4.17.13",
    "@types/history": "^4.7.9",
    "@types/jest": "^27.4.0",
    "@types/jest-environment-puppeteer": "^5.0.0",
    "@types/lodash": "^4.14.178",
    "@types/puppeteer": "^5.4.4",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-flag-icon-css": "^1.0.5",
    "@types/react-helmet": "^6.1.5",
    "@types/react-test-renderer": "^17.0.1",
    "@types/swagger-ui-react": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@umijs/fabric": "^2.8.0",
    "@umijs/openapi": "^1.3.0",
    "@umijs/plugin-blocks": "^2.2.0",
    "@umijs/plugin-esbuild": "^1.4.0",
    "@umijs/plugin-openapi": "^1.3.0",
    "@umijs/preset-ant-design-pro": "^1.3.0",
    "@umijs/preset-dumi": "^1.1.0",
    "@umijs/preset-react": "^1.8.17",
    "@umijs/yorkie": "^2.0.5",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "detect-installer": "^1.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unicorn": "^40.0.0",
    "express": "^4.17.2",
    "gh-pages": "^3.2.3",
    "jest-puppeteer": "^6.0.3",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^12.1.7",
    "mockjs": "^1.0.1-beta3",
    "nyc": "^15.1.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.5.1",
    "puppeteer": "^13.1.2",
    "react-test-renderer": "^17.0.2",
    "standard": "^16.0.4",
    "stylelint": "^14.2.0",
    "stylelint-config-css-modules": "^2.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "typescript": "^4.5.4",
    "umi-serve": "^1.9.10"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
